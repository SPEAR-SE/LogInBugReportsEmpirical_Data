ool_col), 1, 0)) from bool_vect_issue;\n708206\n\nselect sum(if((bool_col == True), 1, 0)) from bool_vect_issue;\n697966\n\nselect sum(if((bool_col is null), 1, 0)) from bool_vect_issue;\n868512\n\nselect sum(if(coalesce(bool_col,false)), 1, 0)) from bool_vect_issue;\n231\n\nselect a.x, count(*) from (select bool_col as x from bool_vect_issue) a group by a.x;\nNULL\t868512\ntrue\t231\n\nselect a.x, count(*) from (select if(bool_col, true, false) x from bool_vect_issue) a group by a.x;\nfalse\t160537\ntrue\t708206","updateAuthor":{"self":"https://issues.apache.org/jira/rest/api/2/user?username=amrk7","name":"amrk7","key":"amrk7","avatarUrls":{"48x48":"https://issues.apache.org/jira/secure/useravatar?avatarId=10452","24x24":"https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=10452","16x16":"https://issues.apache.org/jira/secure/useravatar?size=xsmall&avatarId=10452","32x32":"https://issues.apache.org/jira/secure/useravatar?size=medium&avatarId=10452"},"displayName":"Amruth S","active":true,"timeZone":"Etc/UTC"},"created":"2016-09-14T05:10:51.396+0000","updated":"2016-09-14T05:10:51.396+0000"}],"maxResults":1,"total":1,"startAt":0},"votes":{"self":"https://issues.apache.org/jira/rest/api/2/issue/HIVE-14741/votes","votes":0,"hasVoted":false},"worklog":{"startAt":0,"maxResults":20,"total":0,"worklogs":[]},"customfield_12311820":"0|i33kd3:"}}