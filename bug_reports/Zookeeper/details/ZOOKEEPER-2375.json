ient.java:305)","customfield_10010":null,"timetracking":{},"customfield_12312026":null,"customfield_12312023":null,"customfield_12312024":null,"customfield_12312340":null,"attachment":[{"self":"https://issues.apache.org/jira/rest/api/2/attachment/12790077","id":"12790077","filename":"ZOOKEEPER-2375_01.patch","author":{"self":"https://issues.apache.org/jira/rest/api/2/user?username=yuemeng","name":"yuemeng","key":"yuemeng","avatarUrls":{"48x48":"https://issues.apache.org/jira/secure/useravatar?avatarId=34045","24x24":"https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=34045","16x16":"https://issues.apache.org/jira/secure/useravatar?size=xsmall&avatarId=34045","32x32":"https://issues.apache.org/jira/secure/useravatar?size=medium&avatarId=34045"},"displayName":"yuemeng","active":true,"timeZone":"Etc/UTC"},"created":"2016-02-26T06:27:18.574+0000","size":1972,"mimeType":"text/plain","content":"https://issues.apache.org/jira/secure/attachment/12790077/ZOOKEEPER-2375_01.patch"},{"self":"https://issues.apache.org/jira/rest/api/2/attachment/12790458","id":"12790458","filename":"ZOOKEEPER-2375_02.patch","author":{"self":"https://issues.apache.org/jira/rest/api/2/user?username=yuemeng","name":"yuemeng","key":"yuemeng","avatarUrls":{"48x48":"https://issues.apache.org/jira/secure/useravatar?avatarId=34045","24x24":"https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=34045","16x16":"https://issues.apache.org/jira/secure/useravatar?size=xsmall&avatarId=34045","32x32":"https://issues.apache.org/jira/secure/useravatar?size=medium&avatarId=34045"},"displayName":"yuemeng","active":true,"timeZone":"Etc/UTC"},"created":"2016-02-29T12:00:00.928+0000","size":2026,"mimeType":"text/plain","content":"https://issues.apache.org/jira/secure/attachment/12790458/ZOOKEEPER-2375_02.patch"},{"self":"https://issues.apache.org/jira/rest/api/2/attachment/12789904","id":"12789904","filename":"ZOOKEEPER-2375.patch","author":{"self":"https://issues.apache.org/jira/rest/a