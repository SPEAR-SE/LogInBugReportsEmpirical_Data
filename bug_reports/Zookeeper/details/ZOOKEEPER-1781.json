.\nBut, it may be better to mention this restriction in documentation or add a validation in the source code.","customfield_10010":null,"timetracking":{},"customfield_12312026":null,"customfield_12312023":null,"customfield_12312024":null,"customfield_12312340":null,"attachment":[{"self":"https://issues.apache.org/jira/rest/api/2/attachment/12607062","id":"12607062","filename":"ZOOKEEPER-1781.patch","author":{"self":"https://issues.apache.org/jira/rest/api/2/user?username=bwtakacy","name":"bwtakacy","key":"bwtakacy","avatarUrls":{"48x48":"https://issues.apache.org/jira/secure/useravatar?avatarId=10452","24x24":"https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=10452","16x16":"https://issues.apache.org/jira/secure/useravatar?size=xsmall&avatarId=10452","32x32":"https://issues.apache.org/jira/secure/useravatar?size=medium&avatarId=10452"},"displayName":"Takashi Ohnishi","active":true,"timeZone":"Asia/Tokyo"},"created":"2013-10-06T07:57:01.550+0000","size":6507,"mimeType":"text/plain","content":"https://issues.apache.org/jira/secure/attachment/12607062/ZOOKEEPER-1781.patch"},{"self":"https://issues.apache.org/jira/rest/api/2/attachment/12606988","id":"12606988","filename":"ZOOKEEPER-1781.patch","author":{"self":"https://issues.apache.org/jira/rest/api/2/user?username=bwtakacy","name":"bwtakacy","key":"bwtakacy","avatarUrls":{"48x48":"https://issues.apache.org/jira/secure/useravatar?avatarId=10452","24x24":"https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=10452","16x16":"https://issues.apache.org/jira/secure/useravatar?size=xsmall&avatarId=10452","32x32":"https://issues.apache.org/jira/secure/useravatar?size=medium&avatarId=10452"},"displayName":"Takashi Ohnishi","active":true,"timeZone":"Asia/Tokyo"},"created":"2013-10-05T12:18:21.348+0000","size":2036,"mimeType":"text/plain","content":"https://issues.apache.org/jira/secure/attachment/12606988/ZOOKEEPER-1781.patch"}],"aggregatetimeestimate":null,"customfield_12312341":null,"customf