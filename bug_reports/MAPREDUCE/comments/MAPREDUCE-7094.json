  } finally {
    try {
      fs.delete(systemJobFile.getParent(), true);  // delete submit dir
    } finally {
      localFs.delete(localJobFile, true);              // delete local copy
    }
  }
} catch (IOException e) { 
  LOG.warn("Error cleaning up " + id + ": " + e);
}{code}
